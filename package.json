{
  "name": "patio-inteligente",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4200",
    "start:dev": "ng serve --port=4200",
    "start:tomcat": "ng serve --configuration=hom --port=4200",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build:profile": "ng build --prod --e=hom --aot --stats-json && webpack-bundle-analyzer dist/stats.json",
    "build:spring": "npm run clean:webapp && ng build --prod --aot=true  && npm run publish:webapp:windows && npm run publish:webapp:mac",
    "build:tomcat": "npm run clean:webapp && ng build --prod --aot=true  --base-href /agrotopusyms/ && npm run publish:webapp:windows && npm run publish:webapp:mac",
    "build:hom": "npm run clean:webapp && ng build --prod --aot=true --base-href /agrotopusyms/ && npm run publish:webapp:windows && npm run publish:webapp:mac",
    "rimraf": "rimraf",
    "clean:webapp": "npm run rimraf -- ../webapp",
    "publish:webapp:mac": "copyfiles 'dist/patioInteligente/**/*' -u 2 ../webapp",
    "publish:webapp:windows": "copyfiles dist/patioInteligente/**/* -u 2 ../webapp"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.10",
    "@angular/common": "~11.2.10",
    "@angular/compiler": "~11.2.10",
    "@angular/core": "~11.2.10",
    "@angular/forms": "~11.2.10",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "~11.2.10",
    "@angular/platform-browser-dynamic": "~11.2.10",
    "@angular/router": "~11.2.10",
    "angular2-text-mask": "^9.0.0",
    "jquery": "^3.6.0",
    "ngx-bootstrap": "^7.0.0",
    "ngx-cookie": "^5.0.2",
    "numeral": "^2.0.6",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.6.7",
    "text-mask-addons": "0.7.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.9",
    "@angular/cli": "~11.2.9",
    "@angular/compiler-cli": "~11.2.10",
    "@types/jasmine": "~3.6.0",
    "@types/jquery": "^3.5.6",
    "@types/node": "^12.20.19",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
